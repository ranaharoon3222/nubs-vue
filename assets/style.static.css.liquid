@font-face {
  font-family: SangBleu;
  src: url({{'SangBleuVersailles-Regular.static.woff2'|asset_url}})
      format('woff2'),
    url({{' SangBleuVersailles-Regular.static.woff'|asset_url}}) format('woff');
  font-weight: 400;
  font-style: normal;
}
@font-face {
  font-family: SangBleu;
  src: url({{'SangBleuVersailles-Italic.static.woff2'|asset_url}})
      format('woff2'),
    url({{'SangBleuVersailles-Italic.static.woff '|asset_url}}) format('woff');
  font-weight: 400;
  font-style: italic;
}
@font-face {
  font-family: Suisse;
  src: url({{'SuisseIntl-Regular.static.woff2 '|asset_url}}) format('woff2'),
    url({{' SuisseIntl-Regular.static.woff'|asset_url}}) format('woff');
  font-weight: 400;
  font-style: normal;
}
@font-face {
  font-family: Suisse;
  src: url({{'SuisseIntl-Medium.static.woff2 '|asset_url}}) format('woff2'),
    url({{'SuisseIntl-Medium.static.woff '|asset_url}}) format('woff');
  font-weight: 500;
  font-style: normal;
}
@font-face {
  font-family: swiper-icons;
  src: url({{'swiper-icons.static.woff'|asset_url}}) format('woff');
  font-weight: 400;
  font-style: normal;
}
:root {
  --p-family: 'SangBleu';
  --s-family: 'Suisse';
  --h1-size: 35px;
  --h2-size: 30px;
  --h3-size: 25px;
  --h4-size: 20px;
  --b1-size: 16px;
  --b2-size: 14px;
  --button-size: 12px;
  --p-color: #3c3c3b;
  --s-color: #efebeb;
  --b-color: #fafafa;
  --product-bg-color: #f7f5f5;
  --complimentory-color: #abfeda;
  --text-2-color: #999696;
  --text-3-color: #cccccc;
  --error-color: #ff523f;
  --success-color: #7fc613;
  --warning-color: #ecd23c;
  --info-color: #41b7ff;
  --button-width: 230px;
  --button-height: 32px;
}
img {
  max-width: 100%;
}
body {
  background: #fafafa;
}
.comp-bg {
  background-color: var(--complimentory-color);
}
.p-family {
  font-family: var(--p-family) !important;
}
.s-family {
  font-family: var(--s-family) !important;
}
.button-size {
  font-size: 12px !important;
}
.font-12 {
  font-size: 13px !important;
}
b,
h1,
h2,
h3,
h4,
i,
p,
span {
  font-family: var(--p-family);
  font-weight: 400;
  color: var(--p-color);
}
h1 {
  font-size: var(--h1-size);
  line-height: 51px;
}
h2 {
  font-size: var(--h2-size);
  line-height: 46.5px;
}
h3 {
  font-size: var(--h3-size);
  line-height: 33.7px;
}
h4 {
  font-size: var(--h4-size);
  line-height: 29.38px;
}
h6 {
  font-size: 14px;
  line-height: 20px;
  font-weight: 400;
}
.breadcrumb a.active-item,
.p-color {
  color: var(--p-color);
}
.form-floating > label,
.single-mail,
.text2-color {
  color: var(--text-2-color);
}
@media all and (max-width: 768px) {
  h6 {
    font-size: 12px;
    line-height: 18px;
  }
}
.main-button,
.main-button-dark {
  font-family: var(--s-family);
  font-size: var(--button-size);
  outline: 0;
  border: transparent;
  border-radius: 0;
  display: block;
  text-decoration: none;
  line-height: 32px;
}
.main-button:focus {
  background-color: var(--p-color);
  border-color: transparent;
  box-shadow: none;
}
.main-button-dark:focus {
  background-color: #fff;
  border-color: transparent;
  box-shadow: none;
  color: var(--p-color);
}
.main-button-dark:active,
.main-button-dark:active:focus,
.main-button:active,
.main-button:active:focus {
  box-shadow: none;
  border-color: transparent;
}
.main-button {
  width: var(--button-width);
  height: var(--button-height);
  color: var(--s-color);
  background-color: var(--p-color);
}
.main-button:disabled {
  opacity: 0.7;
  pointer-events: none;
  background-color: #fff;
  border: 1px solid var(--p-color);
}
.main-button:hover {
  background-color: #5f5f5f;
  color: inherit;
}
.main-button:active {
  background-color: var(--text-2-color);
}
.main-button-dark {
  width: var(--button-width);
  height: var(--button-height);
  color: var(--p-color);
  background-color: #fff;
}
.main-button-dark:disabled {
  opacity: 0.7;
  pointer-events: none;
  background-color: var(--p-color);
  border: 1px solid var(--s-color);
  color: var(--s-color);
}
.main-button-dark:hover {
  background-color: var(--s-color);
  color: var(--p-color);
}
.main-button-dark:active {
  background-color: var(--text-2-color);
  color: var(--s-color);
}
.no-pad {
  padding: 0;
}
/* =============
 Header Menu
 ============== */
.navbar-brand {
  background-color: var(--s-color);
  height: 100%;
  display: flex;
  align-items: center;
  padding: 10px 40px;
}
.navbar.navbar-light {
  padding: 0;
  height: 56px;
  font-family: var(--s-family);
  background-color: #fff;
  box-shadow: 0 3px 3px rgba(128, 128, 128, 0.116);
}
.navbar.navbar-light > div {
  height: 100%;
}
.navbar-nav {
  margin-left: 40px;
}
.nav-item > a {
  font-size: var(--b2-size);
  color: var(--p-color) !important;
}
.nav-item {
  margin-right: 12px;
}
.cart-counter {
  background-color: var(--complimentory-color);
  font-size: 10px;
  width: 20px;
  height: 20px;
  border-radius: 100px;
  display: inline-block;
  text-align: center;
  line-height: 20px;
  font-family: var(--s-family);
}
.has-megamenu .dropdown,
.navbar .has-megamenu {
  position: static !important;
}
.has-megamenu .megamenu a {
  text-decoration: none;
  color: var(--p-color);
  font-size: 14px;
}
.has-megamenu .megamenu li {
  list-style: none;
  margin-bottom: 4px;
}
.cs-width {
  max-width: 1300px;
}
#search-box {
  width: 300px;
  left: -17rem;
  top: 50px;
  box-shadow: 0 3px 25px 3px #80808014;
  border-radius: 0;
  height: 48px;
  padding: 0 20px;
  align-items: center;
  font-size: 12px;
}
#search-box.show {
  display: flex;
}
#search-box input {
  border: none;
  outline: 0;
  height: 48px;
  width: -webkit-fill-available;
  color: var(--text-2-color);
}
.right-menu {
  margin-right: 2rem;
}
.has-dropdown .dropdown-menu {
  box-shadow: 0 8px 10px rgb(0 0 0 / 5%);
  border: 0;
  border-radius: 0;
  top: 36px;
}

.mobile_logo {
  background-color: #efebeb;
}
.mm-spn.mm-spn--light {
  color: var(--p-color);
  background: #ffffff;
  font-family: var(--p-family);
}
.mm-ocd.mm-ocd--left {
  top: 56px;
}
.mm-ocd__content {
  width: 100%;
}
.mm-ocd__backdrop {
  z-index: -1;
}

/* =============
 Header Menu
 ============== */

body,
html {
  overflow-x: hidden;
}
.container {
  max-width: 95%;
}
.home-cs-width {
  max-width: 548px;
}
.boder-line {
  border-bottom: 1px solid var(--p-color);
}
.home-2 {
  height: calc(100vh - 240px);
}
.home2-cs-width {
  max-width: 582px;
}
span.slide-line {
  display: block;
  height: 3px;
  width: 18px;
  background: var(--complimentory-color);
  position: absolute;
  right: -42px;
  top: 80px;
}
.custom-slide-pagination {
  position: absolute;
  right: 90px;
  bottom: 40px;
  z-index: 9;
}
.custom-slide-pagination > div {
  display: grid;
  place-content: center;
  height: 300px;
  overflow: auto;
  transition: 0.5s;
}
section#custom-navigation > div::-webkit-scrollbar {
  display: none;
}
.slide-pagination.active-slide {
  opacity: 0.5;
}
.slide-pagination {
  transition: 0.5s;
  color: #fff;
  font-family: var(--s-family);
  text-align: right;
}
.banner-arrow {
  position: absolute;
  bottom: 30px;
  left: 50%;
  transform: translateX(-50%);
}
#slider-wrap {
  position: relative;
}
.home-banner-1 {
  height: calc(100vh - 56px) !important;
}
.c-accordian button {
  display: flex;
  padding-top: 0;
  padding-bottom: 0;
  line-height: 24px;
}
.c-accordian .accordion-item {
  border: 0 solid rgba(0, 0, 0, 0.125);
  pointer-events: none;
  margin-bottom: 1rem;
  background: transparent;
}
.c-accordian button {
  background-color: transparent !important;
}

.c-accordian img {
  order: -1;
  margin-right: 25px;
}
.c-accordian .accordion-button:not(.collapsed) {
  background-color: transparent;
  color: inherit;
  box-shadow: none;
}
.c-accordian .accordion-button::after {
  background-image: url(../../assets/img//plus.svg);
  width: 2rem;
  height: 2rem;
  background-size: 2rem;
}
.c-accordian .accordion-button:not(.collapsed):after {
  background-image: url(../../assets/img//minus.svg);
}
#footer {
  background-color: var(--p-color);
}
#footer .social {
  margin-top: 30px;
}
#footer .social a {
  margin-right: 30px;
}
#footer .copyright {
  font-family: var(--s-family);
  font-size: 12px;
  color: #fff;
  margin-top: 40px;
}
#footer .copyright a {
  display: block;
  color: #fff;
  text-decoration: none;
}
#footer .copyright strong {
  font-weight: 500;
}
.footer_heading {
  font-size: 14px !important;
  color: #fff;
  font-family: var(--s-family);
  font-weight: 400;
  position: relative;
}
a.footer_heading:after {
  content: '\e313';
  font-family: 'Material Icons';
  font-size: 22px;
  position: absolute;
  top: -4px;
  right: 0;
  transform: rotate(180deg);
  transition: 0.3s;
}
a.footer_heading:hover {
  color: #fff;
}
.footer_heading.collapsed:after {
  transform: rotate(0);
}
.footer_link_col a {
  color: #fff;
  display: block;
  text-decoration: none;
  font-size: 12px;
  margin-top: 6px;
}
.breadcrumb a {
  color: #ccc;
  font-family: var(--s-family);
  text-decoration: none;
}
.breadcrumb {
  padding-top: 20px;
  padding-bottom: 20px;
  border-bottom: 1px solid #3c3c3b;
}
.no-border {
  border: none !important;
}
.breadcrumb-item + .breadcrumb-item::before {
  content: var(--bs-breadcrumb-divider, '|');
}
.border-b,
.high-border {
  padding-bottom: 150px;
  border-bottom: 1px solid #3c3c3b;
}
.team-list a {
  position: relative;
  top: 10px;
  text-decoration: none;
  text-transform: uppercase;
  color: var(--p-color);
  border-bottom: 1.5px solid var(--p-color);
  font-size: var(--b1-size);
  padding-bottom: 4px;
}
.team-list {
  padding-left: 0;
  padding-right: 0;
  margin-bottom: 30px;
}
.team-list:nth-of-type(odd) {
  padding-right: 10px;
}
.team-list:nth-last-of-type(2n + 1) {
  padding-right: 0;
  padding-left: 10px;
}
.single-desc {
  background: var(--complimentory-color);
  padding: 20px 55px;
  max-width: 600px;
  margin-top: 90px;
}
.single-desc::before {
  content: url({{'quote.static.png'|asset_img_url:'master'}});
  position: absolute;
  left: 15px;
  top: 17px;
}
.single-desc::after {
  content: url({{'quote2.static.png'|asset_img_url:'master'}});
  position: absolute;
  right: 20px;
  bottom: 15px;
}
.hover_product h5 {
  color: var(--text-2-color);
  font-size: 13px;
}
.hover_product div {
  font-size: 13px;
  position: absolute;
  transition: 0.3s;
  opacity: 0;
}
.hover_product:hover div {
  bottom: 15px;
  left: 6px;
  opacity: 1;
}
.full-border-top {
  border-top: 1.5px solid var(--p-color);
  padding-top: 10px;
  padding-bottom: 10px;
}
.bg-EFEBEB {
  background-color: #efebeb;
}
.product_slider_arrow img {
  padding-right: 15px;
}
.product_slider_arrow img:last-of-type {
  padding-left: 15px;
  padding-right: 0;
}
.swiper-button-disabled {
  opacity: 0.4;
  cursor: not-allowed;
}
.low-border {
  padding-bottom: 50px;
  border-bottom: 1px solid #3c3c3b;
}
.mx-550 {
  max-width: 550px;
}
.mx-600 {
  max-width: 600px;
}
.pad-large {
  padding-top: 80px;
  padding-bottom: 80px;
}
.pad-xl {
  padding-top: 12vw;
  padding-bottom: 12vw;
}
input.form-control {
  border-radius: 0;
  border-color: #8080801f;
}
.form-control:focus {
  border-color: #80808046;
  box-shadow: none;
}
textarea {
  height: 150px !important;
}
.product-price {
  max-width: 170px;
}
#product_toggle {
  cursor: pointer;
}
#product_toggle i {
  right: 0;
  top: 50%;
  transform: translateY(-50%);
}
#product_toggle .dropdown-toggle::after {
  display: none;
}
#product_toggle ul {
  width: 100%;
  border: none;
  box-shadow: 2px 2px 8px rgba(0, 0, 0, 0.1);
  border-radius: 0;
}
.product_description {
  margin-top: 300px;
}
#quantity_picker {
  border: 0.5px solid #3c3c3b;
  width: 30%;
  height: 32px;
  justify-content: space-between;
}
#quantity_picker input::-webkit-inner-spin-button,
#quantity_picker input::-webkit-outer-spin-button {
  -webkit-appearance: none;
  margin: 0;
}
#quantity_picker i.material-icons {
  font-size: 20px;
}
#quantity_picker input {
  width: 35px;
  background: 0 0;
  text-align: center;
}
#quantity_picker input:focus {
  border: none;
  outline: 0;
}
#quantity_picker button {
  display: flex;
  padding: 0 3px;
}
#quantity_picker button:active,
#quantity_picker button:focus {
  box-shadow: none;
}
#add_to_cart {
  width: 70%;
}
#add_to_cart .main-button {
  width: 100%;
}
#add_to_cart .spinner-border {
  display: none;
  color: #fff;
}

.loading #add_to_cart .spinner-border {
  display: inline-block;
}

.slide-4-img {
  height: 80vh;
}
@media all and (min-width: 1420px) {
  .c-accordian .accordion-button {
    font-size: var(--h3-size) !important;
  }
  .c-accordian p {
    font-size: var(--b1-size);
  }
}
@media all and (min-width: 980px) {
  #quantity_picker button {
    padding: 0 10px;
  }
  .space-end {
    margin-right: 15%;
  }
  .c-accordian .accordion-button::after,
  a.footer_heading:after {
    display: none;
  }
}
@media all and (max-width: 980px) {
  .product_description {
    margin-top: 60px;
  }
  .footer_link_col > div {
    border-top: 0.5px solid #999696;
    border-bottom: 0.5px solid #999696;
    padding-top: 10px;
    padding-bottom: 10px;
  }
  .footer_link_col:nth-of-type(odd) > div {
    border-top: 0;
  }
  .footer_link_col a {
    margin: 0;
  }
  .footer_link_col ul {
    margin-top: 10px;
  }
  .footer_link_col ul li {
    margin-bottom: 10px;
  }
  #footer {
    margin-top: 25px;
  }
  .slide-4-img {
    height: 55vh;
  }
  .c-accordian {
    margin-top: 30px;
  }
  .c-accordian img {
    margin-right: 10px;
  }
  .c-accordian .accordion-button {
    padding-left: 0;
    padding-right: 0;
  }
  .c-accordian .accordion-item {
    pointer-events: all;
  }
  .home-2 {
    display: grid;
    justify-content: flex-start;
    align-items: flex-end;
    padding-bottom: 30px;
  }
  .slide-pagination {
    font-size: 0;
    width: 16px;
    height: 3px;
    background: #fafafa;
    margin-left: auto;
    opacity: 0.7;
  }
  .slide-pagination.active-slide {
    background: var(--complimentory-color);
    opacity: 1;
  }
  span.slide-line {
    display: none;
  }
  .custom-slide-pagination {
    position: absolute;
    right: 17px;
    bottom: -58px;
    z-index: 9;
  }
}
.b1-size {
  font-size: 16px !important;
}

/* inner navigation */

div#inner-navigation {
  z-index: 99;
  position: absolute;
  width: 100%;
  top: 50%;
  left: 0;
  transform: translateY(-50%);
}

#inner-navigation .inner-next {
  position: absolute;
  right: 20px;
  top: 0;
  width: 30px;
}
#inner-navigation .inner-prev {
  position: absolute;
  left: 20px;
  top: 0;
  width: 30px;
}
#inner-navigation img {
  display: block;
  margin: auto;
}

/* end inner Navigation */

.sale-badge {
  background: var(--complimentory-color);
  display: inline-block;
  font-size: 12px;
  padding: 4px 8px;
  position: absolute;
  right: 0;
  z-index: 999;
  top: 15px;
}
.sale-badge span {
  font-family: var(--s-family);
}

#product_gallery .swiper-slide {
  cursor: zoom-in;
}
#product_gallery span.swiper-pagination-bullet {
  width: 10px;
  height: 10px;
  background: #f7f5f5;
  border: 1px solid rgb(219, 218, 218);
}
#product_gallery span.swiper-pagination-bullet.swiper-pagination-bullet-active {
  border: 1px solid black;
}

/* end product gallery */

/* cart popup */

#cart_popup[isopened='true'] {
  display: block;
  transition: all 0.5s;
  opacity: 1;
  background: rgba(0, 0, 0, 0.281);
}
.modal-confirm {
  color: #636363;
  max-width: 325px;
  font-size: 14px;
  margin-left: auto;
  margin-right: auto;
}
.modal-confirm .modal-content {
  padding: 20px;
  border-radius: 5px;
  border: none;
}
.modal-confirm .modal-header {
  border-bottom: none;
  position: relative;
  flex-wrap: wrap;
  padding-bottom: 0;
}
.modal-confirm .modal-body {
  padding-bottom: 0;
}
.modal-confirm h4 {
  text-align: center;
  font-size: 26px;
  margin: 10px 0 -15px;
}
.modal-confirm .form-control,
.modal-confirm .btn {
  min-height: 40px;
  border-radius: 3px;
}
.modal-confirm .close {
  position: absolute;
  top: -5px;
  right: -5px;
}
.modal-confirm .modal-footer {
  border: none;
  text-align: center;
  border-radius: 5px;
  font-size: 13px;
}
.modal-confirm .icon-box {
  color: #fff;
  position: absolute;
  margin: 0 auto;
  left: 0;
  right: 0;
  top: -50px;
  width: 65px;
  height: 65px;
  border-radius: 50%;
  z-index: 9;
  background: var(--complimentory-color);
  padding: 15px;
  text-align: center;
  box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.1);
}
.modal-confirm .icon-box i {
  font-size: 29px;
  position: relative;
  top: 3px;
}
.modal-confirm.modal-dialog {
  top: 75px;
}
.modal-confirm .btn {
  color: var(--p-color);
  border-radius: 4px;
  background: var(--complimentory-color);
  text-decoration: none;
  transition: all 0.4s;
  line-height: normal;
  border: none;
  width: 100%;
  font-size: 12px;
}
.modal-confirm .btn:hover,
.modal-confirm .btn:focus {
  color: white;
  background: var(--p-color);
  outline: none;
}

/* cart popup end */
