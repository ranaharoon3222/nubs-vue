/* inner navigation */

div#inner-navigation {
  z-index: 99;
  position: absolute;
  width: 100%;
  top: 50%;
  left: 0;
  transform: translateY(-50%);
}

#inner-navigation .inner-next {
  position: absolute;
  right: 20px;
  top: 0;
  width: 30px;
}
#inner-navigation .inner-prev {
  position: absolute;
  left: 20px;
  top: 0;
  width: 30px;
}
#inner-navigation img {
  display: block;
  margin: auto;
}

/* end inner Navigation */

.sale-badge {
  background: var(--complimentory-color);
  display: inline-block;
  font-size: 12px;
  padding: 4px 8px;
  position: absolute;
  right: 0;
  z-index: 999;
  top: 15px;
}
.sale-badge span {
  font-family: var(--s-family);
}

#product_gallery .swiper-slide {
  cursor: zoom-in;
}
#product_gallery span.swiper-pagination-bullet {
  width: 10px;
  height: 10px;
  background: #f7f5f5;
  border: 1px solid rgb(219, 218, 218);
}
#product_gallery span.swiper-pagination-bullet.swiper-pagination-bullet-active {
  border: 1px solid black;
}

/* end product gallery */

/* cart popup */

#cart_popup[isopened='true'] {
  display: block;
  transition: all 0.5s;
  opacity: 1;
  background: rgba(0, 0, 0, 0.281);
