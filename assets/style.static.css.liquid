@font-face {
  font-family: SangBleu;
  src: url({{'SangBleuVersailles-Regular.static.woff2'|asset_url}})
      format('woff2'),
    url({{' SangBleuVersailles-Regular.static.woff'|asset_url}}) format('woff');
  font-weight: 400;
  font-style: normal;
}
@font-face {
  font-family: SangBleu;
  src: url({{'SangBleuVersailles-Italic.static.woff2'|asset_url}})
      format('woff2'),
    url({{'SangBleuVersailles-Italic.static.woff '|asset_url}}) format('woff');
  font-weight: 400;
  font-style: italic;
}
@font-face {
  font-family: Suisse;
  src: url({{'SuisseIntl-Regular.static.woff2 '|asset_url}}) format('woff2'),
    url({{' SuisseIntl-Regular.static.woff'|asset_url}}) format('woff');
  font-weight: 400;
  font-style: normal;
}
@font-face {
  font-family: Suisse;
  src: url({{'SuisseIntl-Medium.static.woff2 '|asset_url}}) format('woff2'),
    url({{'SuisseIntl-Medium.static.woff '|asset_url}}) format('woff');
  font-weight: 500;
  font-style: normal;
}
@font-face {
  font-family: swiper-icons;
  src: url({{'swiper-icons.static.woff'|asset_url}}) format('woff');
  font-weight: 400;
  font-style: normal;
}

@font-face {
  font-family: Atlas;
  src: url({{'AtlasGrotesk-Medium.static.woff2 '|asset_url}}) format('woff2'),
    url({{'AtlasGrotesk-Medium.static.woff '|asset_url}}) format('woff');
  font-weight: 500;
  font-style: normal;
  font-display: swap;
}

:root {
  --p-family: 'SangBleu';
  --s-family: 'Suisse';
  --h1-size: 35px;
  --h2-size: 30px;
  --h3-size: 25px;
  --h4-size: 20px;
  --b1-size: 16px;
  --b2-size: 14px;
  --b3-size: 12px;
  --button-size: 12px;
  --p-color: #3c3c3b;
  --s-color: #efebeb;
  --b-color: #fafafa;
  --product-bg-color: #f7f5f5;
  --complimentory-color: #abfeda;
  --text-2-color: #999696;
  --text-3-color: #cccccc;
  --error-color: #ff523f;
  --success-color: #7fc613;
  --warning-color: #ecd23c;
  --info-color: #41b7ff;
  --button-width: 230px;
  --button-height: 32px;
}

.font-16 {
  font-size: 16px;
}

.font-14 {
  font-size: 14px;
}

.ls-1 {
  letter-spacing: 1px;
}
img {
  max-width: 100%;
}
body {
  background: #fafafa;
}
.comp-bg {
  background-color: var(--complimentory-color);
}
.p-family {
  font-family: var(--p-family) !important;
}
.s-family,
.s-family * {
  font-family: var(--s-family) !important;
}
html body .material-icons {
  font-family: 'Material Icons' !important;
}
.button-size {
  font-size: 12px !important;
}
.font-12 {
  font-size: 13px !important;
}
b,
h1,
h2,
h3,
h4,
i,
p,
span {
  font-family: var(--p-family);
  font-weight: 400;
  color: var(--p-color);
}
h1 {
  font-size: var(--h1-size);
  line-height: 51px;
}
h2 {
  font-size: var(--h2-size);
  line-height: 46.5px;
}
h3 {
  font-size: var(--h3-size);
  line-height: 33.7px;
}
h4 {
  font-size: var(--h4-size);
  line-height: 29.38px;
}
h6 {
  font-size: 14px;
  line-height: 20px;
  font-weight: 400;
}
.breadcrumb a.active-item,
.p-color {
  color: var(--p-color);
}
.form-floating > label,
.single-mail,
.text2-color,
.text2-color p {
  color: var(--text-2-color);
}
.test {
}
@media all and (max-width: 768px) {
  h6 {
    font-size: 12px;
    line-height: 18px;
  }
}
.main-button,
.main-button-dark {
  font-family: var(--s-family);
  font-size: var(--button-size);
  outline: 0;
  border: transparent;
  border-radius: 0;
  display: block;
  text-decoration: none;
  line-height: 32px;
  letter-spacing: 1px !important;
}
.main-button:focus {
  background-color: var(--p-color);
  border-color: transparent;
  box-shadow: none;
}
.main-button-dark:focus {
  background-color: #fff;
  border-color: transparent;
  box-shadow: none;
  color: var(--p-color);
}
.main-button-dark:active,
.main-button-dark:active:focus,
.main-button:active,
.main-button:active:focus {
  box-shadow: none;
  border-color: transparent;
}
.main-button {
  width: var(--button-width);
  height: var(--button-height);
  color: var(--s-color);
  background-color: var(--p-color);
}
.main-button[disabled='true'] {
  opacity: 0.7;
  pointer-events: none;
  background-color: #fff;
  border: 1px solid var(--text-3-color);
  color: var(--text-2-color);
  font-weight: 400;
}
.main-button:hover {
  background-color: #5f5f5f;
  color: inherit;
}
.main-button:active {
  background-color: var(--text-2-color);
}
.main-button-dark {
  width: var(--button-width);
  height: var(--button-height);
  color: var(--p-color);
  background-color: #fff;
}
.main-button-dark:disabled {
  opacity: 0.7;
  pointer-events: none;
  background-color: var(--p-color);
  border: 1px solid var(--s-color);
  color: var(--s-color);
}
.main-button-dark:hover {
  background-color: var(--s-color);
  color: var(--p-color);
}
.main-button-dark:active {
  background-color: var(--text-2-color);
  color: var(--s-color);
}
.no-pad {
  padding: 0;
}
